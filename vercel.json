{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/contact.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [   
    {
      "src": "/api/contact",
      "dest": "/api/contact"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]    
}
